{
  "Arrow Function": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "ar",
    "body": ["($1) => {$2}"],
    "description": "Arrow Function"
  },

  "Async Arrow Function": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "asa",
    "body": ["async ($1) => {$2};"],
    "description": "Arrow Function"
  },

  "Function": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "fun",
    "body": ["function ${1:name}($2) {\n\t$3\n};"],
    "description": "Function"
  },

  "Async Function": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "asf",
    "body": ["async function ${1:name}($2) {\n\t$3\n};"],
    "description": "Async Function"
  },

  "Tab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "rt",
    "body": "<$1></>",
    "description": "Tab for jsx"
  },

  "Single Tab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "rst",
    "body": ["<$1 />"],
    "description": "Single Tab for jsx"
  },

  "Function Component": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "fnc",
    "body": [
      "import React from 'react';\n\nconst ${1/(.*)/${1:/capitalize}/} = () => {\n\treturn <$2></$2>;\n};\n\nexport default ${1/(.*)/${1:/capitalize}/};\n"
    ],
    "description": "Print Function Component snippet"
  },

  "useEffect": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "uff",
    "body": ["useEffect(() => {", "\t$2\n}, [$1]);"]
  },

  "export default": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "efa",
    "body": "export default $1;"
  },

  "useState": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "uss",
    "body": ["const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2);"],
    "description": "Print useState snippet"
  },

  "console": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "clg",
    "body": [
      "console.${2|log,error,info,warn,trace,debug,table,groupCollapsed,groupEnd,group|}($1);"
    ],
    "description": "Print console.log snippet"
  },

  "comment block": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "cbs",
    "body": ["//#region", "\t$1", "//#endregion"],
    "description": "Print comment block snippet"
  },

  "import": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "imp",
    "body": ["import ${2:module} from '${1:package}';"],
    "description": "Print import snippet"
  },

  "require": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "req",
    "body": ["const ${2:module} = require('${1:package}');"],
    "description": "Print import snippet"
  },

  "useDispatch": {
    "scope": "javascript, javascriptreact, typescript, typescriptreact",
    "prefix": "usd",
    "body": ["const ${1:dispatch} = useDispatch();"],
    "description": "Print useDispatch snippet"
  },

  "useSelector": {
    "scope": "javascript, javascriptreact, typescript, typescriptreact",
    "prefix": "usl",
    "body": [
      "const { $2 } = useSelector<${1:any}, ${1:any}>(({ $2 }) => ({ $2 }))"
    ],
    "description": "Print useSelector snippet"
  }
}
