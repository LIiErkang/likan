{
  "arrowfun": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "arr",
    "body": ["($1) => {$2}"],
    "description": "Arrow Function"
  },

  "tab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "tab",
    "body": "<$1></>",
    "description": "Tab for jsx"
  },

  "stab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "stab",
    "body": ["<$1 />"],
    "description": "Single Tab for jsx"
  },

  "funcom": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "funcom",
    "body": [
      "import React from 'react';\n\nconst $1 = () => {\nreturn <$2></$2>;\n};\n\nexport default $1;\n"
    ],
    "description": "Print Function Component snippets"
  },

  "useEffect": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "uff",
    "body": ["useEffect(() => {", "$2}", "[$1]);"]
  },

  "export default": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "efa",
    "body": "export default $1;"
  },

  "useState snippets": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "usss",
    "body": [
      "const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2);",
      "$0"
    ],
    "description": "Print useState snippets"
  },

  "console.log snippets": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "clg",
    "body": [
      "console.${2|log,error,info,warn,trace,debug,table,groupCollapsed,groupEnd,group|}($1);"
    ],
    "description": "Print console.log snippets"
  },

  "comment block snippets": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "cbs",
    "body": ["//#region", "\t$1", "//#endregion"],
    "description": "Print comment block snippets"
  },

  "import snippets": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "imp",
    "body": ["import ${2:module} from '${1:package}';"],
    "description": "Print import snippets"
  },

  "require snippets": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "req",
    "body": ["const ${2:module} = require('${1:package}');"],
    "description": "Print import snippets"
  }
}
