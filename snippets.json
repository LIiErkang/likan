{
  "arrowfun": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "arr",
    "body": ["async ($1) => {$2}"],
    "description": "Arrow Function"
  },

  "tab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "tab",
    "body": "<$1></>",
    "description": "Tab for jsx"
  },

  "stab": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "stab",
    "body": ["<$1 />"],
    "description": "Single Tab for jsx"
  },

  "funcom": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "funcom",
    "body": [
      "import React from 'react'\n\nconst $1 = () => { return <$2></$2> }\n\nexport default $1;\n"
    ],
    "description": "Quick Write Function Component for jsx"
  },

  "comment": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "comment",
    "body": [
      "/**",
      " * @Author likan",
      " * @Date $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND ",
      " * @FilePath $TM_FILEPATH",
      " * @Software Visual Studio Code",
      " */\n"
    ],
    "description": "Comment file"
  },
  "useEffect": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "useEffect",
    "body": "useEffect(() => {$1}, [$2]);"
  },

  "export default": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "efa",
    "body": "export default $1;"
  }
}
